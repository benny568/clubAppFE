{
  "name": "clubAppFE",
  "version": "1.0.0-beta.1",
  "private": true,
  "description": "Club App front end",
  "keywords": [],
  "scripts": {
    "lint": "tslint -c ./tslint.json \"./src/**/*.ts\" -e \"./src/**/typings.d.ts\" -e \"./src/environments/**\"",
    "ngc": "ngc",
    "reinstall": "rm -rf node_modules tmp deploy dist && npm i",
    "start": "webpack-dev-server --env=dev --progress",
    "rmaot": "rimraf ./aot",
    "rmwebapp": "rimraf ../src/main/webapp/*",
    "cleanjsfiles": "find ./src -name \"*.js\" -exec rm -f {} \\;",
    "cleanmapfiles": "find ./src -name \"*.js.map\" -exec rm -f {} \\;",
    "cleanup": "npm run rmaot && npm run rmwebapp && npm run cleanjsfiles && npm run cleanmapfiles",
    "build:dev": "webpack --env=dev --config webpack.config.js --progress --profile --bail",
    "build:prod": "npm run cleanup && npm run compile:aot && webpack --config webpack.config.js --env=prod --progress",
    "compile:aot": "ngc -p tsconfig-aot.json",
    "build:full": "npm run cleanup && npm run compile:aot && rollup -c rollup-config.js",
    "try": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benny568/clubAppFE.git"
  },
  "license": "MIT",
  "author": "Brendan O'Daly",
  "contributors": [
    "Brendan O'Daly <odalybr@hotmail.com>"
  ],
  "dependencies": {
    "@angular/animations": "^4.0.2",
    "@angular/common": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/platform-server": "^4.0.2",
    "@angular/router": "^4.0.2",
    "@swimlane/ngx-charts": "^5.0.0",
    "core-js": "^2.4.1",
    "corejs": "^1.0.0",
    "d3": "^4.7.4",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.10.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.3.0",
    "showdown": "^1.6.4",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/compiler": "^4.0.2",
    "@angular/compiler-cli": "^4.0.2",
    "@ngtools/webpack": "^1.3.0",
    "@types/hammerjs": "^2.0.34",
    "@types/node": "^7.0.12",
    "@ultimate/aot-loader": "^0.1.15",
    "angular-router-loader": "^0.6.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "codelyzer": "^2.1.1",
    "css-loader": "^0.28.0",
    "ember-cli-inject-live-reload": "^1.6.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jshint": "^2.9.4",
    "postcss-loader": "^1.3.3",
    "primeng": "^2.0.6",
    "raw-loader": "^0.5.1",
    "rollup": "^0.41.6",
    "rollup-plugin-alias": "^1.3.0",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-css-only": "^0.2.0",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "uglify-js": "github:mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "^0.4.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^4.1.0"
  }
}
