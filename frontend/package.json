{
  "name": "clubAppFE",
  "version": "1.0.0-beta.1",
  "private": true,
  "description": "Club App front end",
  "keywords": [],
  "scripts": {
    "rmaot": "rimraf ./dist ./aot",
    "rmwebapp": "rimraf ../src/main/webapp/*",
    "cjs": "find ./src -name \"*.js\" -exec rm -f {} \\;",
    "cmap": "find ./src -name \"*.js.map\" -exec rm -f {} \\;",
    "cjson": "find ./src -name \"*.ngsummary.json\" -exec rm -f {} \\;",
    "caot": "find ./src -name \"*.ngfactory.ts\" -exec rm -f {} \\;",
    "cshim": "find ./src -name \"*.css.shim.ngstyle.ts\" -exec rm -f {} \\;",
    "clean": "npm run rmaot && npm run rmwebapp",
    "build:dev": "npm run clean && webpack --config webpack.config.js --env=dev",
    "build:prod": "npm run clean && webpack --config webpack.config.js --env=prod",
    "serve": "webpack-dev-server --env=prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benny568/clubAppFE.git"
  },
  "license": "MIT",
  "author": "Brendan O'Daly",
  "contributors": [
    "Brendan O'Daly <odalybr@hotmail.com>"
  ],
