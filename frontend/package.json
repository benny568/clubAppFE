{
    "name": "clubAppFE",
    "version": "1.0.0-beta.1",
    "private": true,
    "description": "Club App front end",
    "keywords": [],
    "scripts": {
        "rmaot": "rimraf ./dist ./aot",
        "rmwebapp": "rimraf ../src/main/webapp/*",
        "cleanjsfiles": "find ./src -name \"*.js\" -exec rm -f {} \\;",
        "cleanmapfiles": "find ./src -name \"*.js.map\" -exec rm -f {} \\;",
        "cleanjsonfiles": "find ./src -name \"*.ngsummary.json\" -exec rm -f {} \\;",
        "cleanaotfiles": "find ./src -name \"*.ngfactory.ts\" -exec rm -f {} \\;",
        "clean": "npm run rmaot && npm run rmwebapp && npm run cleanjsfiles && npm run cleanmapfiles && npm run cleanjsonfiles && npm run cleanaotfiles",
        "serve": "http-server . -p 5557",
        "compile": "tsc -p tsconfig.json",
        "rollup": "rollup -f iife -c rollup-config.js -o dist/bundle.es2015.js",
        "es5": "tsc --target es5 --allowJs dist/bundle.es2015.js --out dist/bundle.js",
        "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
        "build:prod": "npm run clean && npm run compile && npm run rollup && npm run es5 && npm run minify",
        "stage1": ""
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/benny568/clubAppFE.git"
    },
    "license": "MIT",
    "author": "Brendan O'Daly",
    "contributors": [
        "Brendan O'Daly <odalybr@hotmail.com>"
    ],
    "dependencies": {
        "@angular/animations": "^4.1.1",
        "@angular/common": "^4.1.1",
        "@angular/core": "^4.1.1",
        "@angular/forms": "^4.1.1",
        "@angular/http": "^4.1.1",
        "@angular/material": "^2.0.0-beta.3",
        "@angular/platform-browser": "^4.1.1",
        "@angular/platform-browser-dynamic": "^4.1.1",
        "@angular/platform-server": "^4.1.1",
        "@angular/router": "^4.1.1",
        "@swimlane/ngx-charts": "^5.1.2",
        "core-js": "^2.4.1",
        "corejs": "^1.0.0",
        "d3": "^4.8.0",
        "hammerjs": "^2.0.8",
        "highlight.js": "^9.11.0",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.3.1",
        "rxjs-es": "^5.0.0-beta.12",
        "showdown": "^1.6.4",
        "zone.js": "^0.8.10"
    },
    "devDependencies": {
        "@angular/compiler": "^4.1.1",
        "@angular/compiler-cli": "^4.1.1",
        "@ngtools/webpack": "^1.3.1",
        "@types/hammerjs": "^2.0.34",
        "@types/node": "^7.0.18",
        "@ultimate/aot-loader": "^0.1.15",
        "angular-router-loader": "^0.6.0",
        "chunk-manifest-webpack-plugin": "^1.1.0",
        "codelyzer": "^3.0.1",
        "css-loader": "^0.28.1",
        "ember-cli-inject-live-reload": "^1.6.1",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.11.1",
        "html-webpack-plugin": "^2.28.0",
        "jshint": "^2.9.4",
        "postcss-loader": "^1.3.3",
        "primeng": "^4.0.0",
        "raw-loader": "^0.5.1",
        "rollup": "^0.41.6",
        "rollup-plugin-alias": "^1.3.1",
        "rollup-plugin-angular": "^0.4.4",
        "rollup-plugin-commonjs": "^8.0.2",
        "rollup-plugin-css-only": "^0.2.0",
        "rollup-plugin-image": "^1.0.2",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-typescript2": "^0.4.1",
        "rollup-plugin-uglify": "^1.0.2",
